<footer id="bottom">
  <div class="wide_links">
    <div class="row">
      <div class='col-sm-4'>
        <span style="color: #888; padding-right: 10px;">
          <%= app_name %>
        </span> 
        <a href="/about">About</a> <span class='text-muted'>|</span>
        <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['full_domain'] %>
          <a href="/pricing">Pricing</a> <span class='text-muted'>|</span>
        <% end %>
        <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['support_url'] %>
          <a href="<%= @domain_overrides['settings']['support_url'] %>">Support</a> <span class='text-muted'>|</span>
        <% end %>
        <a href="/contact">Contact Us</a>
      </div>
      <div class='col-sm-3 col-sm-offset-3' style="text-align: right;">
        <a href="https://github.com/swahlquist/LingoLinq-AAC">Developers</a> <span class='text-muted'>|</span>
        <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['full_domain'] %>
          <a href="/jobs">Jobs</a> <span class='text-muted'>|</span>
        <% end %>
        <a href="/privacy">Privacy</a> <span class='text-muted'>|</span>
        <a href="/terms">Terms</a>
      </div>
      <div class='col-sm-2'>
        <span class="icons">
          <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['blog_url'] %>
            <a href="<%= @domain_overrides['settings']['blog_url'] %>"><img src="data:image/png;base64," alt="{{t "Blog" key='blog'}}"/></a>
          <% end %>
          <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['twitter_url'] %>
            <a href="<%= @domain_overrides['settings']['twitter_url'] %>"><img src="data:image/png;base64," alt="{{t "Twitter" key='twitter'}}"/></a>
          <% end %>
          <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['facebook_url'] %>
            <a href="<%= @domain_overrides['settings']['facebook_url'] %>"><img src="data:image/png;base64," alt="{{t "Facebook" key='facebook'}}"/></a>
          <% end %>
        </span>
      </div>
    </div>
  </div>
  <div class="skinny_links">
    <div class="row">
      <div class='col-sm-3'>
        <span style="color: #888; padding-right: 10px;"><%= app_name %></span> 
        <a href="/about">About</a> <span class='text-muted'>|</span>
        <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['full_domain'] %>
          <a href="/pricing">Pricing</a> <span class='text-muted'>|</span>
        <% end %>
        <a href="<%= @domain_overrides['settings']['support_url'] %>">Support</a> <span class='text-muted'>|</span>
        <a href="/contact">Contact Us</a>
        <span class="icons">
          <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['blog_url'] %>
            <a href="<%= @domain_overrides['settings']['blog_url'] %>"><img src="data:image/png;base64," alt="{{t "Blog" key='blog'}}"/></a>
          <% end %>
          <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['blog_url'] %>
            <a href="<%= @domain_overrides['settings']['twitter_url'] %>"><img src="data:image/png;base64," alt="{{t "Twitter" key='twitter'}}"/></a>
          <% end %>
          <% if @domain_overrides && @domain_overrides['settings'] && @domain_overrides['settings']['twitter_url'] %>
            <a href="<%= @domain_overrides['settings']['facebook_url'] %>"><img src="data:image/png;base64," alt="{{t "Facebook" key='facebook'}}"/></a>
          <% end %>
        </span>
      </div>
    </div>
  </div>
</footer>
